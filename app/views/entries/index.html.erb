<h1>Entries</h1>
<ul>
  <% if @entries.any? %>
    <% @entries.each do |entry| %>
      <li>
        <a href="/entries/<%= entry.id %>"><%= entry.title %></a>
      </li>
    <% end %>
  <% else %>
    <li class="text-muted">No entries yet</li>
  <% end %>
</ul>
<div class="text-center mt-3 mb-4">
  <%= link_to "New Entry", new_entry_path, class: "btn btn-outline-dark" %>
</div>